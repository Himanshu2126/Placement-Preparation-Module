#include <bits/stdc++.h> 
long long getInversions(long long *arr, int n){
    // Write your code here.

    long long ans;

    for (int i = 0; i<n; i++)
    {
      for (int j = 0; j < n; j++)
       {
          if(arr[i]>arr[j] && i<j)
             {
                 ans++;
             }

       }
    }

    return ans;
}
